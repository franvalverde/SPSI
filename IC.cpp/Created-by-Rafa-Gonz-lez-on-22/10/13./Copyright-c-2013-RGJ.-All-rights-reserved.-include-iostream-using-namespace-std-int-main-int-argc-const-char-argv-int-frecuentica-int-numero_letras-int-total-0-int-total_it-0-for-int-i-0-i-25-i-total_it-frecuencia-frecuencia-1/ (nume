#include <iostream>
using namespace std;

int main(int argc, const char * argv[])
{
	int frecuentica;
	int numero_letras;
	int total = 0;
	int total_it = 0;
	
	for (int i=0; i<25; i++){
		total_it = ((frecuencia*(frecuencia-1)) / (numero_letras*(numero_letras-1)));
		total = total + total_it;
	}
	return 0;
}
